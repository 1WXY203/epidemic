n  </div>\r\n</template>\r\n\r\n<script>\r\nimport echarts from \"echarts\";\r\nimport \"../../node_modules/echarts/map/js/china.js\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      \r\n    }\r\n  },\r\n  mounted() {\r\n    \r\n    // axios.get('',(req,res)=>{\r\n    //   console.log(res,'jljlkj')\r\n    // }) \r\n    // console.log('jsdklfjk')\r\n    axios.get('http://localhost:8080/yiqing/')\r\n    .then(function (response) {\r\n      // 处理成功情况\r\n      console.log(response);\r\n      \r\n      \r\n\r\n    // 请求省市数据\r\n      let dataArr = [];\r\n      for(let i =0 ;i<response.data.length; i++){\r\n          let t = response.data[i]\r\n          dataArr.push({ \r\n              name:t.urlname.trim(),\r\n              value:t.number.trim(),\r\n              itemStyle:{\r\n                  areaColor:(function(){\r\n                      let num = t.currentConfirmedCount;\r\n                      if(num ==0){\r\n                          return '#fff'\r\n                      }else if(num>0 && num<10){\r\n                          return '#fdfdcf'\r\n                      }else if(num>=10&&num<20){\r\n                          return '#fe9e83'\r\n                      }else if(num>=20&&num<30){\r\n                          return '#e55a4e'\r\n                      }else{\r\n                          return '#4f070d'\r\n                      }\r\n                  })()\r\n              }\r\n          })\r\n      }\r\n      console.log(dataArr)\r\n      var myChart = echarts.init(document.getElementById(\"container\"));\r\n      var option = {\r\n        tooltip: {\r\n          show: true,\r\n          formatter: function(params) {\r\n            console.log(params);\r\n            return (\r\n              \"<div><p>省：\" +\r\n              params.name +\r\n              \"</p><p>现存确诊：\" +\r\n              params.value +\r\n              \"</p></div>\"\r\n            );\r\n          }\r\n        },\r\n        visualMap:{\r\n            type:'piecewise',\r\n            pieces:[{\r\n                min:1,\r\n                max:10\r\n            },{\r\n                min:10,\r\n                max:20\r\n            },{\r\n                min:20,\r\n                max:30\r\n            },{\r\n                min:30,\r\n                max:40\r\n            }]\r\n        },\r\n        series: [\r\n          {\r\n            name: \"省\",\r\n            type: \"map\",\r\n            map: \"china\",\r\n            roam: false,\r\n            zoom: 1.2,\r\n            aspectScale: 0.75,\r\n            label: {\r\n              normal: {\r\n                show: true,\r\n                textStyle: {\r\n                  color: \"rgba(0,0,0,1)\",\r\n                  fontSize: \"10px\"\r\n                }\r\n              }\r\n            },\r\n            itemStyle: {\r\n              normal: {\r\n                areaColor: \"rgba(0,255,236,0)\",\r\n                borderColor: \"rgba(0,0,0,0.2)\"\r\n              },\r\n              emphasis: {\r\n                areaColor: \"rgba(255,180,0,0.8)\",\r\n                shadowOffsetX: 0,\r\n                shadowOffsetY: 0,\r\n                shadowBlur: 20,\r\n                borderWidth: 0,\r\n                shadowColor: \"rgba(0, 0, 0, 0.5)\"\r\n              }\r\n            },\r\n            data:dataArr\r\n          }\r\n        ]\r\n      };\r\n      myChart.setOption(option, true);\r\n    })\r\n    .catch(function (error) {\r\n      // 处理错误情况\r\n      console.log(error);\r\n    })\r\n    \r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n#container {\r\n  width: 100%;\r\n  min-height: 15rem;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}